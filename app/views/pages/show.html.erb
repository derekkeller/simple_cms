<h1>Page Detail</h1>

<table>
	<tr>
		<th>Subject ID</th>
		<td><%= @page.subject_id %></td>
	</tr>
	<tr>
		<th>Name</th>
		<td><%= @page.name %></td>
	</tr>
	<tr>
		<th>Permalink</th>
		<td><%= @page.permalink %></td>
	</tr>
	<tr>
		<th>Position</th>
		<td><%= @page.position %></td>
	</tr>
	<tr>
		<th>Visible</th>
		<td><%= @page.visible? ? 'Yes' : 'No' %></td>
	</tr>
</table>

<br />
<%= link_to 'Back to pages', pages_path %>